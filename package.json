{
  "name": "audiobook-catalog",
  "version": "0.1.0",
  "description": "UI to list audiobooks by cover with limited organization",
  "main": ".build/server/index.js",
  "repository": "https://github.com/bensaufley/audiobook-catalog",
  "author": "Ben Saufley <contact@bensaufley.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^26.0.20",
    "@types/koa": "^2.11.7",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-send": "^4.1.2",
    "@types/koa-static": "^4.0.1",
    "@types/koa-webpack": "^6.0.1",
    "@types/mongodb": "^3.6.5",
    "@types/node": "^14.14.22",
    "@types/webpack": "^4.41.26",
    "@types/webpack-node-externals": "^2.5.0",
    "@urql/preact": "1.4.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^26.6.3",
    "koa-webpack": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0",
    "webpack-hot-client": "https://github.com/mvattuone/webpack-hot-client#eb1465e278b854ba17cf83796a4f0ff1d24a57cd",
    "webpack-node-externals": "^2.5.2",
    "whatwg-fetch": "^3.5.0"
  },
  "engines": {
    "node": "~14.14.0"
  },
  "resolutions": {
    "webpack-hot-client": "https://github.com/mvattuone/webpack-hot-client#eb1465e278b854ba17cf83796a4f0ff1d24a57cd"
  },
  "scripts": {
    "build": "rimraf .build/{client,server}/* && ROOT_DIR=\"$(pwd)\" TS_NODE_TRANSPILE_ONLY=true webpack --mode none",
    "dev:db": "mongod --bind_ip 0.0.0.0 --dbpath /mongo/db",
    "dev:watch": "ROOT_DIR=\"$(pwd)\" TS_NODE_TRANSPILE_ONLY=true webpack --watch --mode none",
    "dev:serve": "nodemon .build/server/index.js",
    "dev": "npm run build && npm-run-all --parallel dev:db dev:watch dev:serve",
    "lint:es": "eslint .",
    "lint:ts": "tsc --noEmit",
    "lint": "npm-run-all --parallel --aggregate-output --continue-on-error lint:es lint:ts",
    "format": "eslint --fix .",
    "test": "jest"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "graphql": "^15.0.0",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "koa-mount": "^4.0.0",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "mongodb": "^3.6.4",
    "music-metadata": "^7.6.6"
  }
}
