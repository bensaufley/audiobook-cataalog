scalar CoverImage

type Audiobook @entity(
  additionalFields: [
    { path: "meta.checksum", type: "string" }
  ]
) {
  id: ID! @id
  authors: [AudiobookAuthor!]!
  cover: CoverImage @column
  duration: Float! @column
  filepath: String! @column
  genres: [Genre!]! @link
  name: String! @column
  year: Int @column
}
